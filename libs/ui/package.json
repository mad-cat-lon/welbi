{
  "name": "@testwelbi/ui",
  "version": "1.0.0",
  "description": "UI components using Base UI",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun run build:parcel",
    "build:parcel": "parcel build src/index.ts --dist-dir dist || true",
    "dev": "parcel watch src/index.ts --dist-dir dist",
    "clean": "rimraf dist"
  },
  "dependencies": {"playwright":"~1.53.2"},
  "devDependencies": {
    "@parcel/macros": "~2.15.4",
    "@parcel/packager-ts": "^2.15.4",
    "@parcel/transformer-typescript-types": "^2.15.4",
    "@types/react": "latest",
    "@types/bun": "latest",
    "parcel": "latest",
    "typescript": "latest",
    "rimraf": "latest"
  },
  "peerDependencies": {
    "react": "latest",
    "react-dom": "latest"
  }
}
